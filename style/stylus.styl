body
  font-family: 'Lato', sans-serif;
  font-weight: 100
  font-size: 1.75em;
  line-height: 1.55em;
  color: rgb(70, 70, 70)

.first
  background-image: url(../images/header.jpg);
  background-size: cover;
  background-position: center center;
  border-color: red
  position: relative;
  z-index: -10000

.header
  background-color: #fff;
  padding 10px 0
  -webkit-box-shadow: 0px 5px 46px -12px rgba(0,0,0,0.68);
  -moz-box-shadow: 0px 5px 46px -12px rgba(0,0,0,0.68);
  box-shadow: 0px 5px 46px -12px rgba(0,0,0,0.68);

.headerbox
  padding: 30px 0 10px 0;
  font-size: 2em;
  text-transform: uppercase;
  font-weight: 100;
  color: #000;
  background-image: url(../images/85.png);
  position: relative;
  top 22%

.subhead
  font-size: .44em;
  font-weight: 100;
  color: #000;
  margin-top: 10px;

.claim
  font-size: 0.75em;

.box
  margin-top: 100px;
  background-color: #fff;

.boxs
  margin-top 35px;

.textbox
  padding-right: 30px;

.textbox p
  text-align: justify;

.megabox
  height: 10000px;
  background-color #fff;



.window
  height: 750px;
  margin-top 100px;

